<h2>投履历到<%= @job.title %></h2>
<hr>
<div class="col-md-4 col-md-offset-4">
<%= simple_form_for [@job, @resume] do |f| %>
  <%= f.input :content, input_html: { class:"form-control"} %>
  <%= f.input :attachment %>
  <%= f.submit "Submit", class: "btn btn-default",disable_with: "Submiting..."%>

<% end %>
</div>
